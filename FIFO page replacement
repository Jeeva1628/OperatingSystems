#include <stdio.h>

int main() {
    int frames[10], ref[30], n, f, i, j, k = 0, faults = 0, flag;

    printf("Enter number of frames: ");
    scanf("%d", &f);
    printf("Enter length of reference string: ");
    scanf("%d", &n);
    printf("Enter reference string: ");
    for (i = 0; i < n; i++) scanf("%d", &ref[i]);

    for (i = 0; i < f; i++) frames[i] = -1;

    for (i = 0; i < n; i++) {
        flag = 0;
        for (j = 0; j < f; j++) {
            if (frames[j] == ref[i]) { flag = 1; break; }
        }
        if (!flag) {
            frames[k] = ref[i];
            k = (k + 1) % f;
            faults++;
        }
        printf("Frames: ");
        for (j = 0; j < f; j++) printf("%d ", frames[j]);
        printf("\n");
    }
    printf("Total Page Faults = %d\n", faults);
    return 0;
}
