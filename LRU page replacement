#include <stdio.h>

int findLRU(int time[], int n) {
    int i, min = time[0], pos = 0;
    for (i = 1; i < n; i++) {
        if (time[i] < min) { min = time[i]; pos = i; }
    }
    return pos;
}

int main() {
    int frames[10], ref[30], time[10], n, f, i, j, pos, faults = 0, counter = 0;

    printf("Enter number of frames: ");
    scanf("%d", &f);
    printf("Enter length of reference string: ");
    scanf("%d", &n);
    printf("Enter reference string: ");
    for (i = 0; i < n; i++) scanf("%d", &ref[i]);

    for (i = 0; i < f; i++) frames[i] = -1;

    for (i = 0; i < n; i++) {
        int flag = 0;
        for (j = 0; j < f; j++) {
            if (frames[j] == ref[i]) { flag = 1; time[j] = ++counter; break; }
        }
        if (!flag) {
            int empty = -1;
            for (j = 0; j < f; j++) if (frames[j] == -1) { empty = j; break; }
            if (empty != -1) pos = empty;
            else pos = findLRU(time, f);

            frames[pos] = ref[i];
            time[pos] = ++counter;
            faults++;
        }
        printf("Frames: ");
        for (j = 0; j < f; j++) printf("%d ", frames[j]);
        printf("\n");
    }
    printf("Total Page Faults = %d\n", faults);
    return 0;
}
